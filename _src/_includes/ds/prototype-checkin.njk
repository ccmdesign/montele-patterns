---
layout: ds/base.njk
title: Prototye
tags:
  - prototype
icon: 
---
<main class="app-layout">
  <header class="app-layout__header | fixed-bar">
    <div>
      <img class="montele-logo" src="/assets/montele.svg" alt="">
    </div>
    <div class="fixed-bar__stretch">
      <a class="page-title" href="#">
        <h1 class="subject-title" href="" data-icon="{{icon}}">{{header_title}}</h1>
      </a>
    </div>
    <div>
      {% include "patterns/3-modules/full-page-menu/full-page-menu.njk" %}
    </div>
  </header>

  <div class="app-layout__above-main">
    {% include "patterns/3-modules/announcement-bar/announcement-bar.njk" %}
  </div>

  <div class="app-layout__main">
    {{ content | safe }}
  </div>

  <div class="app-layout__above-footer">
    {% include "patterns/4-sections/checkin-section/secondary-menu.njk" %}  
    
  </div>

  <div class="bottom-tabs app-layout__footer | fixed-bar">
    <a class="bottom-tabs__button fixed-bar__stretch | button icon" unstyled href="/pages/prototype/equipamento/"           title="Home"             aria-selected="{% if '/pages/prototype/equipamento/' in page.url %}true{% else %}false{% endif %}"  unstyled>location_on</a>
    <a class="bottom-tabs__button fixed-bar__stretch | button icon" unstyled href="/pages/prototype/equipamento/manutencao" title="Manutenção"       aria-selected="{% if '/pages/prototype/manutencao/' in page.url %}true{% else %}false{% endif %}" unstyled>construction</a>
    <a class="bottom-tabs__button fixed-bar__stretch | button icon" unstyled href="/pages/prototype/equipamento/tech"       title="Tech"             aria-selected="{% if '/pages/prototype/tech/' in page.url %}true{% else %}false{% endif %}" unstyled>stacks</a>
    <a class="bottom-tabs__button fixed-bar__stretch | button icon" unstyled href="/pages/prototype/equipamento/mensagens"  title="Mensagens & Chat" aria-selected="{% if '/pages/prototype/mensagens/' in page.url %}true{% else %}false{% endif %}" unstyled>chat</a>
  </div>
</main>



